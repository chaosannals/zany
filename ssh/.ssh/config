# 客户端保持连接
ServerAliveInterval 50
ServerAliveCountMax 3

Host gitlab(user)
    HostName youserver.com
    User root

Host github(publickey)
    HostName github.com
    User git
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/github

Host reaspberrypi
    HostName 192.168.0.15
    User pi
